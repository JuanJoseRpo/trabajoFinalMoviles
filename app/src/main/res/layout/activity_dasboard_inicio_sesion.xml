<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DasboardInicioSesion">

    <EditText
        android:id="@+id/Etxt_ingresarClave"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="382dp"
        android:ems="10"
        android:hint="Ingresa ContraseÃ±a"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Etxt_ingresarNombre"
        app:layout_constraintTop_toBottomOf="@+id/Etxt_ingresarNombre" />

    <EditText
        android:id="@+id/Etxt_ingresarNombre"
        android:layout_width="wrap_content"
        android:layout_height="0dp"

        android:layout_marginTop="187dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="196dp"
        android:ems="10"
        android:hint="Ingresa Nombre"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/Btn_Ingresar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Btn_Ingresar"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="42dp"
        android:text="Iniciar Sesion"
        app:layout_constraintBottom_toTopOf="@+id/Btn_Registrarse"
        app:layout_constraintStart_toStartOf="@+id/Etxt_ingresarClave"
        app:layout_constraintTop_toBottomOf="@+id/Etxt_ingresarNombre" />

    <Button
        android:id="@+id/Btn_Registrarse"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="126dp"
        android:text="Registrarse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Btn_Ingresar"
        app:layout_constraintTop_toBottomOf="@+id/Btn_Ingresar" />

    <Button
        android:id="@+id/Btn_Volver"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="69dp"
        android:text="Volver"
        app:layout_constraintBottom_toTopOf="@+id/Etxt_ingresarNombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>